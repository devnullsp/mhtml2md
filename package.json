{
	"name": "mhtml2md",
	"displayName": "mhtml2md",
	"description": "convert mhtml a md",
	"version": "1.0.1",
	"engines": {
		"vscode": "^1.35.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:mhtml2md"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [{
				"command": "mhtml2md",
				"title": "Convertir mhtml a md"
		}],
		"menus": {
			"explorer/context": [{
				"when": "resourceFilename =~ /.*mht$/",
				"command": "mhtml2md"
			}]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"n-readlines": "^1.0.0",
		"xml2js": "^0.4.19",
		"xmldom": "^0.1.27",
		"xpath": "0.0.27"
	}
}
